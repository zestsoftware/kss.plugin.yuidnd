/* kss rules for drag and drop
 */

#firstlist li:yuidnd-dragstart {
    evt-yuidnd-dragstart-targetIds: first,second;
}

#secondlist li:yuidnd-dragstart {
    evt-yuidnd-dragstart-targetIds: second;
}

#firstlist:yuidnd-drop(first) {
    evt-yuidnd-drop-action: 'order';
}

#secondlist:yuidnd-drop(second) {
    evt-yuidnd-drop-action: 'order';
    action-client: alert;
}

